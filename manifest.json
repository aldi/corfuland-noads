{
  "name": "Corfuland NoAds",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Block all annoying ads from Corfuland.gr",
  "icons": {"48": "logo_48.png", "128": "logo_128.png" },
  "homepage_url": "https://github.com/aldid/corfuland-noads",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["corfulandblocker.css"],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_title": "No ads active!"
  }
}